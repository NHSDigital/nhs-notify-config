{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "properties": {
    "data": {
      "additionalProperties": false,
      "properties": {
        "configType": {
          "enum": [
            "client",
            "campaign",
            "client-subscription",
            "channel-supplier",
            "supplier-quota",
            "feature-flag",
            "specification"
          ],
          "type": "string"
        },
        "id": {
          "type": "string"
        }
      },
      "required": [
        "configType",
        "id"
      ],
      "type": "object"
    },
    "datacontenttype": {
      "const": "application/json",
      "type": "string"
    },
    "dataschema": {
      "pattern": "^https:\\/\\/notify\\.nhs\\.uk\\/events\\/schemas\\/config-removed-1.\\d+\\.\\d+\\.json$",
      "type": "string"
    },
    "dataschemaversion": {
      "pattern": "^1\\.\\d+\\.\\d+$",
      "type": "string"
    },
    "id": {
      "type": "string"
    },
    "plane": {
      "const": "control",
      "type": "string"
    },
    "source": {
      "type": "string"
    },
    "specversion": {
      "const": "1.0",
      "type": "string"
    },
    "subject": {
      "type": "string"
    },
    "time": {
      "type": "string"
    },
    "type": {
      "const": "uk.nhs.notify.config.ConfigRemoved",
      "type": "string"
    }
  },
  "required": [
    "dataschema",
    "dataschemaversion",
    "type",
    "id",
    "source",
    "specversion",
    "subject",
    "time",
    "datacontenttype",
    "plane",
    "data"
  ],
  "type": "object"
}
